{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.svg",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/github-device-code",
      "dest": "/api/github-device-code.js"
    },
    {
      "src": "/api/github-device-token", 
      "dest": "/api/github-device-token.js"
    },
    {
      "src": "/api/github-graphql",
      "dest": "/api/github-graphql.js"
    },
    {
      "src": "/api/github-proxy/(.*)",
      "dest": "/api/github-proxy.js"
    },
    {
      "src": "/api/auth-set-token",
      "dest": "/api/auth-set-token.js"
    },
    {
      "src": "/api/auth-clear-token",
      "dest": "/api/auth-clear-token.js"
    },
    {
      "src": "/api/auth-status",
      "dest": "/api/auth-status.js"
    },
    {
      "src": "/api/test-proxy",
      "dest": "/api/test-proxy.js"
    },
    {
      "src": "/script.js",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate",
        "Pragma": "no-cache",
        "Expires": "0"
      },
      "dest": "/script.js"
    },
    {
      "src": "/favicon.svg",
      "headers": {
        "Content-Type": "image/svg+xml",
        "Cache-Control": "public, max-age=86400"
      },
      "dest": "/favicon.svg"
    },
    {
      "src": "/favicon.ico",
      "headers": {
        "Content-Type": "image/svg+xml",
        "Cache-Control": "public, max-age=86400"
      },
      "dest": "/favicon.svg"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "GITHUB_CLIENT_ID": "Ov23li2VVjfGHdt11COT"
  }
}